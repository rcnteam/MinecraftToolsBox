<StackPanel Orientation="Horizontal"
            x:Class="MinecraftToolsBoxSDK.Location"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
            xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
            xmlns:local="clr-namespace:MinecraftToolsBoxSDK"
            mc:Ignorable="d" Width="345">
    <Label x:Name="X" Content="X:" FontSize="16" VerticalContentAlignment="Center"/>
    <local:LocBox x:Name="LocX" Width="85" FontSize="16" ToolTip="不要输入~号" VerticalContentAlignment="Center"/>
    <Label x:Name="Y" Content="Y:" Width="25" FontSize="16" Margin="8,0,0,0" VerticalContentAlignment="Center"/>
    <local:LocBox x:Name="LocY" Width="85" FontSize="16" ToolTip="不要输入~号" VerticalContentAlignment="Center"/>
    <Label x:Name="Z" Content="Z:" Width="25" FontSize="16" Margin="8,0,0,0" VerticalContentAlignment="Center"/>
    <local:LocBox x:Name="LocZ" Width="85" FontSize="16" ToolTip="不要输入~号" VerticalContentAlignment="Center"/>
</StackPanel>
