    <tools:LocBox x:Name="LocY" HorizontalAlignment="Left" Margin="135,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="25" Width="84" FontSize="16"/>
    <tools:LocBox x:Name="LocZ" HorizontalAlignment="Left" Margin="247,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="25" Width="85" FontSize="16"/>
    <Label Content="X:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    <Label Content="Y:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="112,0,0,0"/>
    <Label Content="Z:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="227,0,0,0"/>
