    <tools:LocBox x:Name="LocY" HorizontalAlignment="Left" Margin="172,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="25" Width="70" FontSize="16" ToolTip="不要输入~号"/>
    <tools:LocBox x:Name="LocZ" HorizontalAlignment="Left" Margin="262,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="25" Width="70" FontSize="16" ToolTip="不要输入~号"/>
    <Label Content="X:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="61,0,0,0"/>
    <Label Content="Y:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="152,0,0,0"/>
    <Label Content="Z:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="242,0,0,0"/>
    <CheckBox x:Name="tilde" Content="相对坐标" HorizontalAlignment="Left" Margin="-3,3,0,0" VerticalAlignment="Top" FontSize="11"/>
