            <Custom:Tile x:Name="BlocksB" HorizontalAlignment="Left" Height="62" Margin="0" VerticalAlignment="Top" Width="56" BorderBrush="#00000000" ToolTip="建筑方块" Click="UpdateItemPick"/>
